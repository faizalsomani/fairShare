{
    "manifest_version": 2,
    "name": "Fair Share",
    "version": "1.0",
    "description": "Easy way to manage shared economy.",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "18": "icon18.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
